% import config
% swf_path = ('{0}/fonts/static/fonts.swf').format(config.FP_SUBDIR)
<script>
  var d = document.createElement('div');
  d.id = 'flc';
  document.getElementById('fp').appendChild(d);
  $(document).ready(function(){
    $("#flc").flash({"src": "{{swf_path}}", "width": "1", "height": "1", "swliveconnect": "true", "id": "flh", "name": "flh"},{update: false});
  });
</script>

<!--font metrics-->
<style>
#content {
  line-height: 1;
	width: 40em;
	padding: 1em;
	min-width: 400px;
	margin-left: auto;
	margin-right: auto;
}
.testbox div {
	text-align: center;
}
.testbox span {
	font-size: 10000%;
}
.fontloadbox {
	width: 72px;
	height: 72px;
	border: 2px rgba(0, 0, 0, 0) solid;
	vertical-align: bottom;
	font-family: fontload;
	font-size: 64px;
	display: inline-block;
}
@font-face {
	font-family: unifont;
	/* This is unifont/unifont_fastfp-7.0.03.ttf. */
	src: url("data:application/x-font-ttf;base64,") format("truetype");
}
@font-face {
	font-family: linuxlibertine;
	/* This is linuxlibertine/LinLibertine_Rah_fastfp.ttf. */
	src: url("data:application/x-font-ttf;base64,") format("truetype");
}
@font-face {
	font-family: fontload;
	/* This special font file has zero-width characters U+0041 and U+1D538.
	   We use it to detect when the web fonts have finished downloading.
	   It's built from fontload.ttx.
	   https://github.com/Pomax/Font.js/blob/master/Font.js
	   http://processingjs.nihongoresources.com/the_smallest_font/ */
	src: url("data:application/x-font-ttf;base64,") format("truetype");
}
#span-default {
}
#span-sans-serif {
	font-family: sans-serif;
}
#span-serif {
	font-family: serif;
}
#span-monospace {
	font-family: monospace;
}
#span-cursive {
	font-family: cursive;
}
#span-fantasy {
	font-family: fantasy;
}
#span-standard {
	font-family: linuxlibertine, unifont;
}
/* Special one-family style for Tor Browser, which limits you to only the first matching web font.
   https://gitweb.torproject.org/tor-browser.git/commitdiff/fc24b518cfba6310e1bdfa7d1aadeffaf9e86a2c#patch10
   The unifont font only covers the BMP. */
#span-unifont {
	font-family: unifont;
}
code {
	padding: 0.2em;
	background-color: blanchedalmond;
	border: 1px gray solid;
}
</style>
<div id=content>
  <div id=fontsloading>
    <div class=fontloadbox><span id=fontload-unifont>A</span></div>&nbsp;<div class=fontloadbox><span id=fontload-linuxlibertine>A</span></div>
  </div>
  <div class=testbox>
    <div id=div-default><span id=span-default></span></div>
    <div id=div-sans-serif><span id=span-sans-serif></span></div>
    <div id=div-serif><span id=span-serif></span></div>
    <div id=div-monospace><span id=span-monospace></span></div>
    <div id=div-cursive><span id=span-cursive></span></div>
    <div id=div-fantasy><span id=span-fantasy></span></div>
    <div id=div-standard><span id=span-standard></span></div>
    <div id=div-unifont><span id=span-unifont></span></div>
  </div>
</div>